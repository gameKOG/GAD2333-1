using UnityEngine;
using System.Collections;

public class weekTwoAssignmentOne : MonoBehaviour
{
	public string playerName = ("");		//The text I enter for playerName never gets used.

	// Use this for initialization
	void Start ()
	{
	}

	public void Main()
	{
		SayHello(playerName);
		SayGoodbye();
	}

	public void SayHello(string pName)
	{
		print ("Hello " + pName + "!");
	}
	public void SayGoodbye()
	{
		print ("Goodbye!");
	}
	// Update is called once per frame
	void Update () {
	
	}
}
